{
  "name": "the-grid",
  "productName": "Grid",
  "proxy": "http://localhost:8000",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@electron/remote": "^2.0.10",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.14.17",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "concurrently": "^8.2.0",
    "cross-env": "^7.0.3",
    "electron-is-dev": "^2.0.0",
    "electron-log": "^4.4.8",
    "fs": "^0.0.1-security",
    "javascript-color-gradient": "^2.4.4",
    "react": "^18.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-router-dom": "^6.6.1",
    "react-scripts": "5.0.1",
    "serve": "^13.0.2",
    "styled-components": "^6.1.0",
    "util": "^0.12.5",
    "wait-on": "^7.0.1",
    "web-vitals": "^2.1.4",
    "yarn": "^1.22.19"
  },
  "main": "public/main.js",
  "homepage": "./",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron:serve": "concurrently -k \"cross-env BROWSER=none npx yarn start\" \"npx yarn electron:start\"",
    "electron:build": "yarn build && electron-builder -c.extraMetadata.main=build/main.js",
    "electron:start": "wait-on tcp:3000 && electron ."
  },
  "build": {
    "extends": null,
    "appId": "com.thegrid",
    "win": {
      "icon": "public/grid_logo.ico"
    },
    "mac": {
      "icon": "public/grid_logo.icns"
    },
    "files": [
      "dist/**/*",
      "build/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "extraResources": [
      {
        "from": "../requirements.txt",
        "to": "./requirements.txt"
      },
      {
        "from": "../backend",
        "to": "./backend"
      },
      {
        "from": "../process_files",
        "to": "./process_files"
      },
      {
        "from": "../main2.py",
        "to": "./"
      },
      {
        "from": "../installPackages.py",
        "to": "./"
      },
      {
        "from": "public/installPackages.html",
        "to": "./"
      },
      {
        "from": "public/errorInstall.html",
        "to": "./"
      },
      {
        "from": "public/styles.css",
        "to": "./"
      },
      {
        "from": "public/grid_logo.png",
        "to": "./"
      }
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@iconify/react": "^4.1.1",
    "electron": "^25.2.0",
    "electron-builder": "^23.0.3"
  }
}
